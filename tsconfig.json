{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "jsx-async-runtime",
		"module": "ESNext",
		"moduleResolution": "Node",
		"skipLibCheck": true,
		"target": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
